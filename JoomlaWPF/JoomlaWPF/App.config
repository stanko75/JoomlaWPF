<?xml version="1.0" encoding="utf-8" ?>
<configuration>

  <connectionStrings>
    <add name="JoomlaCon"
      connectionString="
        SERVER=myServer;
        DATABASE=myDb;
        UID=myUid;
        PASSWORD=myPass;
        Convert Zero Datetime=True
